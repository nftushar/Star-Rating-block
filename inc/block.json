{
  "apiVersion": "1",
  "name": "b-blocks/text-path",
  "title": "Text Path",
  "description": "Gutenberg editing with easy-to-use, responsive text-path  more.",
  "category": "media",
  "keywords": [
    "text-path",
    "Rating",
    "Gutenberg"
  ],
  "textdomain": "text-path",
  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "cId": {
      "type": "string",
      "default": ""
    },
    "svgPath": {
      "type": "string",
      "default": "M0 50 A50 50, 0, 1, 1, 100 50"
    },
    
    "rating": {
      "type": "number",
      "default": 5.0
    },
    "iconStyle": {
      "type": "string",
      "default": "solid"
    },
    "prefix": {
      "type": "string",
      "default": ""
    },
    "gap": {
      "type": "string",
      "default": "10px"
    },
    "alignment": {
      "type": "string",
      "default": "center"
    },
    "textTypo": {
      "type": "object",
      "default": {
        "fontSize": "16px"
      }
    },
    "strokeColor": {
      "type": "string",
      "default": "red"
    },
    "textColor": {
      "type": "string",
      "default": "#7a7a7a"
    },
    "textShadow": {
      "type": "array",
      "default": []
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "example": {
    "attributes": {
      "preview": true,
      "columns": {
        "desktop": 1,
        "tablet": 1,
        "mobile": 1
      }
    }
  },
  "editorScript": "file:../dist/editor.js"
}